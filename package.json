{
  "name": "shlink-api-spec-ui",
  "description": "Web UI to load Shlink's sync and async API specs",
  "author": "Alejandro Celaya Alastru√©",
  "readmeFilename": "README.md",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shlinkio/shlink.io"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.ts,.tsx src",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint:css:fix": "npm run lint:css -- --fix",
    "start": "vite serve --host=0.0.0.0",
    "build": "tsc --noEmit && vite build"
  },
  "dependencies": {
    "@asyncapi/react-component": "^1.0.0-next.41",
    "@vitejs/plugin-react": "^3.1.0",
    "classnames": "^2.3.1",
    "compare-versions": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.0",
    "react-router-dom": "^6.8.2",
    "swagger-ui": "4.13.1",
    "swagger-ui-react": "4.13.1",
    "vite": "^4.1.4"
  },
  "devDependencies": {
    "@shlinkio/eslint-config-js-coding-standard": "~2.0.2",
    "@shlinkio/stylelint-config-css-coding-standard": "~1.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.18",
    "@types/react-headroom": "^2.2.2",
    "@types/swagger-ui-react": "^4.11.0",
    "eslint": "^8.23.0",
    "eslint-webpack-plugin": "^3.2.0",
    "postcss": "^8.4.16",
    "stylelint": "^14.11.0",
    "typescript": "^4.8.3"
  }
}
